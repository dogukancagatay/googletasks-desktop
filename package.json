{
  "name": "googletasks-desktop",
  "version": "0.1.3",
  "description": "A desktop app for Google Tasks, using Electron like container",
  "main": "src/main.js",
  "scripts": {
    "start": "electron .",
    "package": "electron-packager . \"Google Tasks\" --platform=darwin --arch=x64 --electron-version=8.2.3 --out=./build --overwrite --icon=./src/images/shortcut.icns --asar"
  },
  "repository": "https://github.com/dogukancagatay/googletasks-desktop",
  "keywords": [
    "Google",
    "Tasks",
    "Task",
    "Electron",
    "Desktop",
    "Windows"
  ],
  "author": "Doğukan Çağatay <dcagatay@gmail.com>",
  "license": "GNU",
  "devDependencies": {
    "electron": "^8.2.3",
    "electron-packager": "^14.2.1"
  }
}
